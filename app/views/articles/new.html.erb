<div class="contents">
  <div class="container">
  <%= form_for @article do |f| %>
    <h3>
      投稿する
    </h3>
    <%= f.text_field :title, placeholder: 'title', style: 'width: 100%;' %>
    <%= f.text_area :text, placeholder: 'text', style: 'width: 100%;height: 300px;' %>
    <%= button_tag type: "submit", class: 'btn' do %>SEND<% end %>
  <% end %>
  </div>
</div>
